<PropertyList>
    
    <path>F.Panel.ac</path>

    <animation>
    <type>pick</type>
    <object-name>cylinder.008</object-name>
    <object-name>cylinder.009</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/gear/gear-down</property>
      </binding>
    </action>
  </animation>
  
  <animation>
    <type>translate</type>
    <object-name>cylinder.008</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0.000 </dep></entry>
      <entry><ind> 1 </ind><dep> -0.021 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>cylinder.009</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0.000 </dep></entry>
      <entry><ind> 1 </ind><dep> -0.021 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

</PropertyList>