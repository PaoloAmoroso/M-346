        <object-name>nav1</object-name>
        <object-name>nav1-label</object-name>
        <object-name>nav2</object-name>
        <object-name>nav2-label</object-name>
        <ambient>
            <red>0.0</red>
            <green>1.0</green>
            <blue>0.0</blue>
        </ambient>
        <diffuse>
            <red>0.0</red>
            <green>0.1</green>
            <blue>0.0</blue>
        </diffuse>
        <emission>
            <red>0.3</red>
            <green>0.8</green>
            <blue>0.3</blue>
        </emission> 
    </animation>
    
