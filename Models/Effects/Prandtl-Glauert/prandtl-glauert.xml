<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <model>
    <name>prandtl-glauertNOALS</name>
    <path>Classic/prandtl-glauert.xml</path>
  </model>

  <animation>
    <type>select</type>
    <object-name>prandtl-glauertNOALS</object-name>
    <condition>
      <not><property>/sim/rendering/shaders/skydome</property></not>
    </condition>
  </animation>

  <model>
    <name>prandtl-glauertALS</name>
    <path>ALS/prandtl-glauert.xml</path>
  </model>

  <animation>
    <type>select</type>
    <object-name>prandtl-glauertALS</object-name>
    <condition>
      <property>/sim/rendering/shaders/skydome</property>
    </condition>
  </animation>

  <model>
    <name>prandtl-glauertHDR</name>
    <path>HDR/prandtl-glauert.xml</path>
  </model>

  <animation>
    <type>select</type>
    <object-name>prandtl-glauertHDR</object-name>
    <condition>
      <property>/sim/rendering/hdr/hdr-enabled</property>
    </condition>
  </animation>

</PropertyList>
