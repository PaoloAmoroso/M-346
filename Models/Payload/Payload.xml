     <!-- Leonardo M-346FT/FA Master WEAPONS -->
     <animation>
    <type>select</type>
    <object-name>AIM-9M-L</object-name>
    <object-name>AIM-9M-R</object-name>
    <object-name>MK-82-1L</object-name>
    <object-name>MK-82-2L</object-name>
    <object-name>MK-82-1R</object-name>
    <object-name>MK-82-2R</object-name>
    <object-name>GBU-12-L</object-name>
    <object-name>GBU-12-R</object-name>
    <object-name>Tank-L</object-name>
    <object-name>Tank-C</object-name>
    <object-name>Tank-R</object-name>
    <object-name>Litening</object-name>
    <object-name>Sniper</object-name>
    <object-name>Cannon</object-name>
    <condition>
        <and>
         <property>sim/model/hide-Racks</property>
        </and>
    </condition>
  </animation>

	<!-- Pylon 1L - Pylon 1 Wingtip Left -->
	<model>
		<name>AIM-9M-L</name>
		<path>Aircraft/M-346/Models/Payload/AIM-9M.ac</path>
		<offsets>
			<x-m> 1.6699</x-m>
			<y-m>-4.8815</y-m>
			<z-m> 0.3094</z-m>
			<roll-deg>270</roll-deg>
		</offsets>
	</model>
	
	<animation>
		<type>select</type>
		<object-name>AIM-9M-L</object-name>
		<condition>
		     <equals>
			 <property>sim/weight[6]/selected</property>
			 <value>AIM-9M</value>
			</equals>
		</condition>
	</animation>

     <animation>
          <type>select</type>
          <object-name>AIM-9M-L</object-name>
           <condition>
             <not><property>controls/signal1</property></not>
           </condition>
     </animation>

